    <![CDATA[SELECT Distinct HeaderEO.DIVISION_ID, 
       HeaderEO.HEADER_ID, 
       HeaderEO.DOC_NUM, 
       HeaderEO.MARKER_DATE, 
       HeaderEO.SALESORDER_ID, 
       HeaderEO.CAD_OPERATOR_ID, 
       HeaderEO.CAD_OP_NAME, 
       HeaderEO.CUSTOMER_ID, 
       HeaderEO.STYLE, 
       HeaderEO.BPO, 
       HeaderEO.REMARKS, 
       HeaderEO.LAST_UPDATE_DATE, 
       HeaderEO.LAST_UPDATED_BY, 
       HeaderEO.CREATION_DATE, 
       HeaderEO.CREATED_BY, 
       HeaderEO.LAST_UPDATE_LOGIN, 
       HeaderEO.ATTRIBUTE1, 
       HeaderEO.ATTRIBUTE2, 
       HeaderEO.ATTRIBUTE3, 
       HeaderEO.ATTRIBUTE4, 
       HeaderEO.ATTRIBUTE5, 
       HeaderEO.ATTRIBUTE6, 
       HeaderEO.ATTRIBUTE7, 
       HeaderEO.ATTRIBUTE8, 
       HeaderEO.ATTRIBUTE9, 
       HeaderEO.ATTRIBUTE10, 
       HeaderEO.ATTRIBUTE11, 
       HeaderEO.ATTRIBUTE12, 
       HeaderEO.ATTRIBUTE13, 
       HeaderEO.ATTRIBUTE14, 
       HeaderEO.ATTRIBUTE15,
       HeaderEO.Buyer,
       LinesEO.BUYER_ID,
       LinesEO.STYLE_NAME,
       LinesEO.ATTRIBUTE1 SEASON,
(select known_as
from hz_parties hp,
hz_cust_accounts hca
where 
    hp.party_id =  hca.party_id
    and hp.KNOWN_AS is not null
    AND LinesEO.BUYER_ID=HCA.CUST_ACCOUNT_ID
    AND ROWNUM=1) Buery_ShortCode,
 LinesEO.Marker_Plan_Number,
 LinesEO.Marker_Id,
 LinesEO.POC_ID,
LinesEO.SEASON Buyer_New,
LinesEO.ATTRIBUTE1 Season_New
FROM MNJ_MFG_MARHER_INFO_H HeaderEO,MNJ_MFG_MARHER_INFO_L LinesEO
where LinesEO.HEADER_ID=HeaderEO.HEADER_ID
  and HeaderEO.DIVISION_ID = :p_org_id
order by HeaderEO.HEADER_ID desc]]>
  </SQLQuery>
